    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/ui": "^0.7.2",
    "@vue/apollo-composable": "^4.0.0-alpha.12",
    "apollo-link-error": "^1.1.13",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "bson-objectid": "^1.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.4",
