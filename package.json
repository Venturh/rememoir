    "eslint-plugin-vue": "^7.6.0",
    "postcss": "^8.2.6",
    "prettier": "^2.1.2",
    "tailwindcss": "^2.0.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.69",
    "vite-plugin-components": "^0.6.7",
    "vite-plugin-voie": "^0.7.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
