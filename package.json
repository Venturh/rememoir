    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-vue-apollo": "^2.3.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
