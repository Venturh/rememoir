    "events": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "loadash": "^1.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3",
    "nodemailer": "^6.4.16",
    "pouchdb-adapter-idb": "^7.2.2",
    "reflect-metadata": "^0.1.13",
    "rxdb": "^9.13.0",
    "rxjs": "^6.6.3",
    "subscriptions-transport-ws": "^0.9.18",
    "type-graphql": "^1.1.1",
