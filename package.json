{
  "name": "vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "docker-compose up",
    "web:dev": "vite ",
    "web:gen": "graphql-codegen -r dotenv/config --config config.yml",
    "web:build:nocheck": "vite build",
    "web:build": "vuedx-typecheck . && vite build",
    "web:serve": "vite preview",
    "api:dev": "ts-node-dev --transpile-only --respawn  -r dotenv/config ./api/index.ts",
    "api:docker": "ts-node-dev --poll --respawn --transpile-only --respawn  -r dotenv/config api/index.ts",
    "postinstall": "node vue-apollo-patch.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.9",
